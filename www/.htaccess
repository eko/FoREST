# Rewrite
Options +FollowSymlinks

RewriteEngine On
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.*)$ index.php [L,QSA]

# Authentication

AuthType Basic
AuthGroupFile /dev/null
AuthName "Restricted Access"
AuthUserFile /var/www/foREST/www/.htpasswd
Require valid-user
